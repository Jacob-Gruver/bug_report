{
  "name": "bug_report",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run setup:client && npm run setup:server && npm run build:client",
    "setup:client": "cd bug_report.client && npm i",
    "setup:server": "cd bug_report.server && npm i",
    "build:client": "npm run --prefix bug_report.client build",
    "start": "node bug_report.server/index.js"
  },
  "engines": {
    "node": "^14.15.4"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "serve": "^11.3.2"
  }
}
